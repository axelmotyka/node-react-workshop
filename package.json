{
  "name": "node-react-workshop",
  "version": "1.0.0",
  "description": "Example project that includes a React + Redux frontend and a NodeJS + KOA backend",
  "dependencies": {
    "bluebird": "^3.5.2",
    "js": "^0.1.0",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "sqlite3": "^4.0.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  },
  "scripts": {
    "client": "cd frontend && npm start",
    "server": "nodemon --inspect ./backend/src/server/server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "author": "Axel Motyka",
  "license": "ISC"
}
